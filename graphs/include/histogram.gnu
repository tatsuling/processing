set style data histograms

set style fill solid 1.00 noborder
set style line 12 lc rgb '#808080' lt 12 lw 2
set style line 13 lc rgb '#808080' lt 13 lw 1
set grid ytics mytics back ls 12, ls 13

set style line 11 lc rgb '#808080' lt 1
set border 3 back ls 11
set tics nomirror
set bars 0.5

set xtics border in scale 0,0 nomirror norotate

set yrange [0:]

set xtics tc rgbcolor '#000000'
set ytics tc rgbcolor '#000000'
